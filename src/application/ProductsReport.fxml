<AnchorPane xmlns="http://javafx.com/javafx/19.0.2"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.SalesReportController">

	<BarChart fx:id="salesBarChart" layoutX="400.0"
		layoutY="100.0" prefHeight="500.0" prefWidth="700.0">
		<xAxis>
			<CategoryAxis label="Product ID" />
		</xAxis>
		<yAxis>
			<NumberAxis label="Sale Rate (%)" />
		</yAxis>
	</BarChart>

	<Button text="Back" layoutX="50.0" layoutY="750" fx:id="backbtn"
		styleClass="update-btn" stylesheets="@application.css"
		onAction="#handleBackButton" />
</AnchorPane>
